{
  "name": "@textlint-ja/textlint-rule-preset-foreign-language-writing-helper",
  "version": "1.0.0",
  "description": "A helper for textlint-rule-preset-foreign-language-writing ",
  "keywords": [
    "helper",
    "textlint"
  ],
  "homepage": "https://github.com/textlint-ja/textlint-rule-preset-foreign-language-writing/tree/master/packages/textlint-rule-preset-foreign-language-writing-helper/",
  "bugs": {
    "url": "https://github.com/textlint-ja/textlint-rule-preset-foreign-language-writing/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint-ja/textlint-rule-preset-foreign-language-writing.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/textlint-rule-preset-foreign-language-writing-helper.js",
  "types": "lib/textlint-rule-preset-foreign-language-writing-helper.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.7.4",
    "sudachi-synonyms-dictionary": "^4.1.0"
  },
  "peerDependencies": {
    "sudachi-synonyms-dictionary": "*"
  },
  "publishConfig": {
    "access": "public"
  }
